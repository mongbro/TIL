<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" 
xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Calculator">
   <top>
      <HBox prefHeight="100.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="500.0">
               <children>
                  <TextField prefHeight="40.0" prefWidth="50.0" text="수식" />
                  <Label fx:id="TEXT1" prefHeight="40.0" prefWidth="200.0" />
                  <TextField prefHeight="40.0" prefWidth="50.0" text="결과" />
                  <Label fx:id="TEXT2" prefHeight="40.0" prefWidth="200.0" />
               </children>
            </HBox>
         </children>
      </HBox>
   </top>
   <center>
      <GridPane prefHeight="400.0" prefWidth="500.0" BorderPane.alignment="CENTER">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="NUM9" mnemonicParsing="false" prefHeight="100.0" prefWidth="125.0" text="9" GridPane.columnIndex="2">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
            <Button fx:id="NUM8" mnemonicParsing="false" prefHeight="100.0" prefWidth="125.0" text="8" GridPane.columnIndex="1">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
            <Button fx:id="NUM7" mnemonicParsing="false" prefHeight="100.0" prefWidth="125.0" text="7">
